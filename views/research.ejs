                    <div class="probootstrap-teacher text-center probootstrap-animate">
                      <figure class="media">
                        <img src="/photo/<%=student.photo %>" alt="student-1" class="img-responsive">
                      </figure>
                      <div class="text">
                          <h3><%=student.name%></h3>
                          <p><b>Branch:</b> <%=student.department%><br><b>PassOut Year:</b><%=student.PassOutYear%></p>
                      
                      </div>
                    </div>
                  </div>

              <%}%>

